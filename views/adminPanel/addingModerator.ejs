 <div class="card-body">
   <div class="card-title">Yangi Moderator qo'shish</div>
   <hr>
    <form name="addingModerator"  method="POST" action="/addingModerator">
   <div class="form-group row">
    <label for="input-26" class="col-sm-2 col-form-label">Ism</label>
    <div class="col-sm-10">
    <input type="text" name="name" id="ism" class="form-control form-control-rounded" id="input-26" placeholder="Ismingizni kiriting"  required>
    </div>
  </div>
  <div class="form-group row">
    <label for="input-27" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
    <input type="email" name="email" id="email" class="form-control form-control-rounded" id="input-27" placeholder="Elektron manzilingizni kiriting"  required>
    </div>
  </div>
  <div class="form-group row">
    <label for="input-28" class="col-sm-2 col-form-label">Telefon</label>
    <div class="col-sm-10">
    <input type="text" id="telefon" class="form-control form-control-rounded" id="input-28" placeholder="Telefon raqamingizni kiriting" name="telephone" required>
    </div>
  </div>
  <div class="form-group row">
    <label for="input-29" class="col-sm-2 col-form-label">Parol</label>
    <div class="col-sm-10">
    <input type="password" id="parol" class="form-control form-control-rounded" id="input-29" placeholder="Parolni kiriting" name="password" required>
    </div>
  </div>
  <div class="form-group row">
    <label for="input-30" class="col-sm-2 col-form-label">Parolni tasdiqlash</label>
    <div class="col-sm-10">
    <input type="password" id="parol2" class="form-control form-control-rounded" id="input-30" placeholder="Parolni tekshiring" name="rePassword" required>
    </div>
  </div>
  <div class="form-group row">
    <label for="input-30" class="col-sm-2 col-form-label">Roleni tasdiqlash</label>
    <div class="col-sm-10">
      <input value="moderator" type="text" id="form" class="form-control" name="role" required hidden>
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
    <div class="icheck-material-primary">
    <input type="checkbox" id="user-checkbox6" checked="">
    <label for="user-checkbox6">Eslab qol</label>
    </div>
    </div>
  </div>
   <div class="form-group row">
    <label class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
    <button onclick="add()" type="button" class="btn btn-primary btn-round px-5"><i class="icon-lock"></i> Registratsiya</button>
    </div>
  </div>
  </form>
  <!-- <button onclick="add()">OK</button> -->
