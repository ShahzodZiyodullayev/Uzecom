 <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js"></script>
 
 
<script>
    function add(){
      axios({
        method: "POST",
        url: '/addingModerator',
        data: {
          name: document.getElementById('ism').value,
          email: document.getElementById("email").value,
          telephone: document.getElementById("telefon").value,
          password: document.getElementById("parol").value,
          rePassword: document.getElementById("parol2").value,
          role: document.getElementById("form").value
        }
      })
      console.log(document.getElementById('ism').value)
      console.log(document.getElementById("email").value)
      console.log(document.getElementById("telefon").value)
      console.log(document.getElementById("parol").value)
      console.log(document.getElementById("parol2").value)
      console.log(document.getElementById("form").value)
    }
</script>
