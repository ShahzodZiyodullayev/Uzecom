                        <td>
                            <div class="amount">
                                <button><i class="fas fa-minus"></i></button>
                                <input type="text" value="1 ШТ">
                                <button><i class="fas fa-plus"></i></button>
                            </div>
                        </td>
                        <td>
                            <p class="price">150 000 <%=translation.som%>.</p>
                        </td>
                        <td>
                            <div class="color"></div>
                        </td>
                        <td>
                            <div class="size">
                                <h3>M</h3>
                            </div>
                        </td>
                        <td>
                            <div class="delete"><a href="#"><i class="fas fa-times"></i></a></div>
                        </td>
                    </tr>

                </table>
                <br>
                <hr>
                <table class="allProduct">
                    <tr>
                        <th><h1><%=translation.jami%> :</h1></th>
                        <th><span>3 шт</span></th>
                        <th> <p class="price" style="margin: 0; margin-left: 40px;">370 000 <%=translation.som%>.</p></th>
                    </tr>
                </table>
            </div>


            <div class="dannie">
