                        <td>
                            <div class="amount">
                                <button><i class="fas fa-minus"></i></button>
                                <input type="text" value="1 ШТ">
                                <button><i class="fas fa-plus"></i></button>
                            </div>
                        </td>
                        <td>
                            <p class="price">120 000 <%=translation.som%>.</p>
                        </td>
                        <td>
                            <div class="color"></div>
                        </td>
                        <td>
                            <div class="size">
                                <h3>XXL</h3>
                            </div>
                        </td>
                        <td>
                            <div class="delete"><a href="#"><i class="fas fa-times"></i></a></div>
                        </td>
                    </tr>
                    <tr>
                        <td style="width: 34%;">
                            <div class="hiring">
                                <div class="hiringImage">
                                    <img src="./images/three.png" alt="">
                                </div>
                                <div class="hiringText">
                                    <p>ZARA <%=translation.koylak%>
                                    </p>

                                    <span>
                                        <%=translation.kontrastli%>
                                    </span>
                                </div>
